class lab{
private:
	char name[40];
	int menbers;
	char address[10];
public:
	void SetName(char *n);
	void SetNum(int n);
	void ChangeAd (char *n);
	void InMem(int n);
	void OutMem(int n);
	lab(char,int,char);
};
void lab::SetName(char*ss)
{
		strcpy(name,ss);
}

void lab::SetNum(int n)
{
	menmbers=n;
}

void ChangeAd(char *ss)
{
		strcpy(address,ss);
}

void InMem(int n)
{
		members+=n;
}

void OutMem(int n)
{
		menmbers -=n;
}

lab::lab(char n="nishi", int m=10, char ad="552-4"){
		name=n; menmbers=m;address= ad;
}


